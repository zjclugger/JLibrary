<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zjclugger.component">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name="com.zjclugger.component.module.upload.BillUploadActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.zjclugger.component.picture.MediaPreviewActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.zjclugger.component.ui.FragmentSupportActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
    </application>

</manifest>

